<div id="container-fluid">
    <form id="form">
        <h3 class="h3-title">Dejános tú Consulta:</h3>
        <br>

        <div class="form-group" id="form-group">
            <label for="name">Nombre y Apellido</label>
            <br />
            <div id="input-group">
                <div class="input-group-addon"></div>
                <input type="text" id="name" placeholder="Ingrese su nombre y apellido" />
            </div>
        </div>

        <div class="form-group">
            <label for="email">Correo Electronico</label>
            <br />
            <div class="input-group">
                <div class="input-group-addon"></div>
                <input type="email" id="email" placeholder="Ingrese Correo Electronico " />
            </div>

        </div>
        <div class="form-group">
            <label for="cephone">Teléfono:</label>
            <br />
            <div class="input-group">
                <div class="input-group-addon"></div>
                <input type="int" id="number" placeholder="Ingrese el número" />
            </div>
        </div>

        <div class="form-group">
            <label for="adress">Dirección</label>
            <br />
            <div class="input-group">
                <div class="input-group-addon"></div>
                <input type="adress" id="adress" placeholder="Ingrese su Dirección" />
            </div>
        </div>

        <div class="form-group">
            <label for="cp">Codigo Postal</label>
            <br />
            <div class="input-group">
                <div class="input-group-addon"></div>
                <input type="int" id="number-cp" placeholder="Ingrese el Codigo Postal" />
            </div>

        </div>
        <button type="button" class="btn btn-info">Enviar</button>
</div>